<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link href="https://fonts.googleapis.com/css?family=Darker+Grotesque:400,700&display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Noto+Sans:400,700&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    <link rel="stylesheet" href="css/stylev2.css">
    <link rel="stylesheet" type="text/css" href="//cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.css"/>
<!--    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.13.0/js/all.min.js">-->
    <title>Version 2</title>
</head>
<body>
<!--header & menu-->
<header class="w-100 deactiveHeader" id="header">
    <nav class="navbar navbar-expand-xl navbar-expand-lg navbar-expand-md navbar-expand-sm">
        <div class="container-fluid">
            <a href="#" class="navbar-branch">
                <img src="img/logo.png" alt="" height="40px">
            </a>
            <div class="collapse navbar-collapse">
                <ul class="navbar-nav ml-auto">
                    <% if (displayName){ %>
                        <li class="nav-item">
                            <div class="userHover">
                                Welcome, <%= displayName %>
                                <ul class="profiles">
                                    <li>
                                        <a href="#" class="profileLink">Thông tin</a>
                                    </li>
                                    <li>
                                        <form method="get" action="/user/logout">
                                            <input type="submit" class="profileLink" value="Đăng xuất">
                                        </form>
                                    </li>
                                </ul>
                            </div>
                        </li>
                    <% } else {%>
                        <li class="nav-item">
                            <a href="./user/login" class="nav-link">Đăng nhập</a>
                        </li>
                        <li class="nav-item">
                            <a href="./user/register" class="nav-link">Đăng kí</a>
                        </li>
                    <% } %>
                    <li class="nav-item menu-items-special">
                        <button class="btn-menu" id="btn-menu" onclick="menufx(this)">
                            <div id="bar-1"></div>
                            <div id="bar-2"></div>
                            <div id="bar-3"></div>
                        </button>
                    </li>
                </ul>
            </div>
        </div>
    </nav>
    <div class="menu-full-screen" id="menu-full-screen">
        <div>
            <ul class="menu-list">
                <li class="menu-items">
                    <a href="/" class="">Trang chủ</a>
                </li>
                <li class="menu-items">
                    <a href="/" class="">Thông tin</a>
                </li>
                <li class="menu-items">
                    <a href="/" class="">Blog</a>
                </li>
                <li class="menu-items">
                    <a href="/" class="">Liên hệ</a>
                </li>
            </ul>
        </div>
    </div>
</header>
<!--    slider-->
<section class="welcome-block">
    <div class="banner-place" id="banner-slider">
        <div class="slide-item">
            <div class="overlay">
                <img src="img/banner/banner-web-design.png" alt="" class="img-slider">
            </div>
        </div>
        <div class="slide-item">
            <div class="overlay">
                <img src="img/banner/banner-web-design.png" alt="" class="img-slider">
            </div>

        </div>

    </div>
</section>
<!--main-->
<section class="body-place">
    <div class="wrapper-main">
        <div class="container-fluid ">
            <div class="body-main">
                <h1 class="body-title">Tài liệu mới</h1>
                <div class="math">
                    <form action="" class="form-search">
                        <div class="search-subject">
                            <hr class="rule">
                            <div class="sel sel--superman">
                                <select name="select-superpower" id="select-superpower">
                                    <option value="" disabled>Chọn lớp</option>
                                    <% if(grade) grade.forEach((val)=>{ %>
                                        <option value=<%= val %>><%= val %></option>
                                    <% }) %>
                                </select>
                            </div>

                            <div class="sel sel--superman">
                                <select name="select-superpower" id="select-superpower">
                                    <option value="" disabled>Chọn môn học</option>
                                    <% if(subjects) subjects.forEach((val)=>{ %>
                                        <option value=<%= val %>><%= val %></option>
                                    <% }) %>
                                    <option value="Khác">Khác</option>
                                </select>
                            </div>
                        </div>
                        <div class="btn-search-block">
                            <input type="submit" class="btn-search" value="Xem ngay!">
                        </div>
                    </form>

                    <div class="endShow">
                        <div>
                            <a href="#" class="readmore">Read more... </a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
</section>

<script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>

<script type="text/javascript" src="//cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.min.js"></script>

<script src="js/main.js"></script>
</body>
</html>